    }
  },
  "schema": {
    "SystemID": "Unique identifier (FP_SEED_XXXX for seed-based)",
    "protein_name": "Common name of the protein",
    "variant": "Genetic variant (e.g., wild-type, A206K) - NULL if unknown",
    "family": "Protein family (e.g., GFP-like, Calcium, Dopamine)",
    "is_biosensor": "1 if biosensor, 0 if regular FP",
    "uniprot_id": "UniProt accession - NULL if not found",
    "pdb_id": "PDB structure ID - NULL if not found",
    "excitation_nm": "Excitation wavelength (nm) - NULL if not available",
    "emission_nm": "Emission wavelength (nm) - NULL if not available",
    "temperature_K": "Measurement temperature (Kelvin) - NULL if not specified",
    "pH": "pH condition - NULL if not specified",
    "contrast_ratio": "Raw contrast value as reported in source",
    "contrast_ci_low": "Lower bound of 95% CI - NULL if not available",
    "contrast_ci_high": "Upper bound of 95% CI - NULL if not available",
    "contrast_source": "measured | computed | none",
    "condition_text": "Textual description of measurement conditions",
    "source_refs": "DOI, PMCID, or database reference",
    "license_source": "License of source data",
    "measure_type": "fold_change | delta_F_F0 | percent_change | dynamic_range | brightness_ref",
    "contrast_normalized": "Normalized to ΔF/F₀ ratio (fold→fold-1, %→/100)",
    "contrast_quality_tier": "A (with CI/n/SD) | B (measured, no stats) | C (computed) | NULL (none)"
  },
  "normalization_rules": {
    "contrast_to_delta_F_F0": {
      "fold_change": "ΔF/F₀ = fold - 1 (e.g., 10x → 9.0)",
      "delta_F_F0": "Already normalized (keep as is)",
      "percent_change": "ΔF/F₀ = percent / 100 (e.g., 50% → 0.5)",
      "dynamic_range": "Assumed already in ratio form",
      "brightness_ref": "Not a contrast, set to NULL"
    },
    "quality_tiers": {
      "A": "Measured with CI, n, or SD explicitly stated",
      "B": "Measured from literature, no explicit statistics",
      "C": "Computed from QY×ε or other proxies",
      "NULL": "No contrast data"
    }
  },
  "licenses": {
    "UniProt": "CC BY 4.0",
